{
    "language": "ru-RU",
    "title": "Python. За 3 часа",
    "plugins": ["katex","hide-published-with","download-pdf-link","target-logo"],
    "pluginsConfig": {
      "download-pdf-link": {
        "base": "https://www.gitbook.com/book/poppy-project/poppy-docs/",
        "label":  "Загрузить PDF"
      },
      "sharing": {
            "facebook": true,
            "twitter": true,
            "google": false,
            "weibo": false,
            "instapaper": false,
            "vk": true,
            "all": [
                "vk", "facebook", "google", "twitter",
                "weibo", "instapaper"
            ]
        },
        "target-logo": {
            "url": "http://www.example.com",
            "src": "data:image/png;base64,",
            "style": "background: none; width:120px; height:120px"
        }
    }
}
